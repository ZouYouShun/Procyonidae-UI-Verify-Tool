{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@procyonidae/api-interfaces": ["libs/api-interfaces/src/index.ts"],
      "@procyonidae/browser/home/feature": [
        "libs/browser/home/feature/src/index.ts"
      ],
      "@procyonidae/browser/home/services": [
        "libs/browser/home/services/src/index.ts"
      ],
      "@procyonidae/browser/screen/feature/recorder": [
        "libs/browser/screen/feature/recorder/src/index.ts"
      ],
      "@procyonidae/browser/screen/feature/screenshot": [
        "libs/browser/screen/feature/screenshot/src/index.ts"
      ],
      "@procyonidae/browser/screen/feature/shell": [
        "libs/browser/screen/feature/shell/src/index.ts"
      ],
      "@procyonidae/browser/settings/feature/account": [
        "libs/browser/settings/feature/account/src/index.ts"
      ],
      "@procyonidae/browser/settings/feature/shell": [
        "libs/browser/settings/feature/shell/src/index.ts"
      ],
      "@procyonidae/browser/settings/feature/snippets": [
        "libs/browser/settings/feature/snippets/src/index.ts"
