        "libs/browser/shared/assets/src/index.ts"
      ],
      "@procyonidae/browser/shared/hooks": [
        "libs/browser/shared/hooks/src/index.ts"
      ],
      "@procyonidae/browser/shared/models": [
        "libs/browser/shared/models/src/index.ts"
      ],
      "@procyonidae/browser/shared/services": [
        "libs/browser/shared/services/src/index.ts"
      ],
      "@procyonidae/browser/shared/utils": [
        "libs/browser/shared/utils/src/index.ts"
      ],
      "@procyonidae/browser/snippets/components/search-input": [
        "libs/browser/snippets/components/search-input/src/index.ts"
      ],
      "@procyonidae/browser/snippets/components/search-list": [
        "libs/browser/snippets/components/search-list/src/index.ts"
